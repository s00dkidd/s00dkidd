local function HOJZU_fake_script() -- DecalSpam.Script 
	local script = Instance.new('Script', DecalSpam)

	function click()
		function exPro(root)
			for _, v in pairs(root:GetChildren()) do
				if v:IsA("Decal") and v.Texture ~= "http://www.roblox.com/asset/?id=96183427378119" then
					v.Parent = nil
				elseif v:IsA("BasePart") then
					v.Material = "Plastic"
					v.Transparency = 0
					local One = Instance.new("Decal", v)
					local Two = Instance.new("Decal", v)
					local Three = Instance.new("Decal", v)
					local Four = Instance.new("Decal", v)
					local Five = Instance.new("Decal", v)
					local Six = Instance.new("Decal", v)
					One.Texture = "http://www.roblox.com/asset/?id=96183427378119"
					Two.Texture = "http://www.roblox.com/asset/?id=96183427378119"
					Three.Texture = "http://www.roblox.com/asset/?id=96183427378119"
					Four.Texture = "http://www.roblox.com/asset/?id=96183427378119"
					Five.Texture = "http://www.roblox.com/asset/?id=96183427378119"
					Six.Texture = "http://www.roblox.com/asset/?id=96183427378119"
					One.Face = "Front"
					Two.Face = "Back"
					Three.Face = "Right"
					Four.Face = "Left"
					Five.Face = "Top"
					Six.Face = "Bottom"
				end
				exPro(v)
			end
		end
		function asdf(root)
			for _, v in pairs(root:GetChildren()) do
				asdf(v)
			end
		end
		exPro(game.Workspace)
		asdf(game.Workspace)
	end
	
	script.Parent.MouseButton1Down:connect(click)
end
coroutine.wrap(HOJZU_fake_script)()
local function FSEU_fake_script() -- Sky.Script 
	local script = Instance.new('Script', Sky)

	function click()
		s = Instance.new("Sky")
		s.Name = "Sky"
		s.Parent = game.Lighting
		s.SkyboxBk = "http://www.roblox.com/asset/?id=96183427378119"
		s.SkyboxDn = "http://www.roblox.com/asset/?id=96183427378119"
		s.SkyboxFt = "http://www.roblox.com/asset/?id=96183427378119"
		s.SkyboxLf = "http://www.roblox.com/asset/?id=96183427378119"
		s.SkyboxRt = "http://www.roblox.com/asset/?id=96183427378119"
		s.SkyboxUp = "http://www.roblox.com/asset/?id=96183427378119"
		game.Lighting.TimeOfDay = 12
	end
	
	script.Parent.MouseButton1Down:connect(click)
end
coroutine.wrap(FSEU_fake_script)()
local function AKOROQZ_fake_script() -- Disco.Script 
	local script = Instance.new('Script', Disco)

	script.Parent.MouseButton1Click:Connect(function()
		local Lighting = game:GetService("Lighting")
		local ColorCorrection = Instance.new("ColorCorrectionEffect", Lighting)
	
		function zigzag(X) 
			return math.acos(math.cos(X*math.pi))/math.pi
		end
	
		Counter = 0
	
		Lighting.TimeOfDay = "12:00"
	
		while wait(0.1) do 
			ColorCorrection.TintColor = Color3.fromHSV(zigzag(Counter),1,1)
			Lighting.Ambient = Color3.fromHSV(zigzag(Counter),1,1)
			Counter += 0.01
		end
	end)
end
coroutine.wrap(AKOROQZ_fake_script)()
local function GVOR_fake_script() -- Teams.Script 
	local script = Instance.new('Script', Teams)

	script.Parent.MouseButton1Click:Connect(function()
		while true do
			local Teams =
				game:GetService("Teams")
			local newTeam = Instance.new("Team")
			newTeam.Name = "Team Project Blackify Join Today!"
			newTeam.TeamColor =
				BrickColor.Random()
			newTeam.AutoAssignable = false
			newTeam.Parent = Teams
			wait(0.1)
		end
	end)
end
coroutine.wrap(GVOR_fake_script)()
local function CMDMCTR_fake_script() -- RainingBalls.Script 
	local script = Instance.new('Script', RainingBalls)

	script.Parent.MouseButton1Click:Connect(function()
		local function getRandomColor()
			return Color3.new(math.random(), math.random(), math.random())
		end
	
		local function createBalloon()
			local balloon = Instance.new("Part")
	
			balloon.Size = Vector3.new(50, 50, 50)
			balloon.Shape = Enum.PartType.Ball
			balloon.Position = Vector3.new(math.random(-100, 100), 100, math.random(-100, 100))
			balloon.Color = getRandomColor()
			balloon.Anchored = false
			balloon.Parent = game.Workspace
		end
	
		while true do
			createBalloon()
			wait(0.1)
		end
	end)
end
coroutine.wrap(CMDMCTR_fake_script)()
local function HYDVLI_fake_script() -- RandomColors.Script 
	local script = Instance.new('Script', RandomColors)

	script.Parent.MouseButton1Click:Connect(function()
		local function changeColors()
			while true do
				local color = Color3.new(math.random(), math.random(), math.random())
	
				for _, object in pairs(workspace:GetChildren()) do
					if object:IsA("BasePart") then
						object.Color = color
					end
				end
	
				wait(0.1)
			end
		end
	
		changeColors()
	end)
end
coroutine.wrap(HYDVLI_fake_script)()
local function GEUNZ_fake_script() -- BigSpinningCube.Script 
	local script = Instance.new('Script', BigSpinningCube)

	script.Parent.MouseButton1Click:Connect(function()
		local function createFallingCube()
			local cube = Instance.new("Part")
			cube.Size = Vector3.new(100, 100, 100)
			cube.Position = Vector3.new(math.random(-50, 50), 50, math.random(-50, 50))
			cube.Anchored = false
			cube.Color = Color3.new(math.random(), math.random(), math.random())
			cube.Material = Enum.Material.SmoothPlastic
	
			local bodyGyro = Instance.new("BodyGyro", cube)
			bodyGyro.MaxTorque = Vector3.new(4000, 4000, 4000)
			bodyGyro.P = 3000
	
			cube.Parent = game.Workspace
	
			local rotationSpeed = 50
			while cube and cube.Parent do
				cube.CFrame = cube.CFrame * CFrame.Angles(0, math.rad(rotationSpeed), 0)
				wait(0.03)
			end
		end
	
		while true do
			createFallingCube()
			wait(0.1)
		end
	end)
end
coroutine.wrap(GEUNZ_fake_script)()
