	local script = Instance.new('Script', Neonify)

	--NO SKIDDING THIS SCRIPT!--
	
	script.Parent.MouseButton1Click:Connect(function()
		local function setLightingToPink()
			local lighting = game:GetService("Lighting")
	
			lighting.Ambient = Color3.fromRGB(255, 105, 180)
			lighting.Brightness = 1000
			lighting.OutdoorAmbient = Color3.fromRGB(255, 105, 180)
		end
	
		setLightingToPink()
	end)
end
