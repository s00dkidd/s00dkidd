	-- Weapon usage (edit to liking)
	local equip = function()
		equipped = true
		if attacking then
			repeat
				wait()
			until not attacking
		end
		attacking = true
		if not equipped then
			attacking = false
			return
		end
		LW.Part1 = larm
		RW.Part1 = rarm
		LS.Part1 = nil
		RS.Part1 = nil
		attacking = false
		setpose("idle", .085)
	end
	local unequip = function()
		equipped = false
		if attacking then
			repeat
				wait()
			until not attacking
		end
		if equipped then
			return
		end
		setpose("none", .1)
		wait(.65)
		if equipped then
			return
		end
		LS.Part1 = larm
		RS.Part1 = rarm
		LW.Part1 = nil
		RW.Part1 = nil
	end
