				magic.lightning((cane.CFrame*caneEnd).p, hit, 10, 10, "Cyan", 10, 10, 10, charfx)
				if prt and prt.Parent:FindFirstChild("Humanoid") then
					damage(prt, 50, 70, 100, "Knockdown", RP, 0, 0, random(0, 1), nil, true)
