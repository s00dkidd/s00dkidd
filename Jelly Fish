			hum.WalkSpeed = 16
			attacking = false
			music:Resume()
			sound2222233:Stop()
			sound2222224:Stop()
			sound222223444:Stop()
			sound22222243:Stop()
		end
		if key == "v" then
			music:Stop()
			sound222222 = Instance.new("Sound",char)
			sound222222.SoundId = "rbxassetid://949916584"
			sound222222.Volume = 30
			sound222222:Play()
			attacking = true
			print("They Shall Go...")
			hum.WalkSpeed = 12
